{
  "name": "irshad-center-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:fix": "next lint --fix && prettier --write .",
    "lint:strict": "eslint --max-warnings=0 src",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "typecheck": "tsc --noEmit --incremental false",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "studio": "prisma studio",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "prepare": "husky install",
    "cleanup": "npm run format && npm run lint -- --fix && npm run type-check",
    "cleanup-stripe": "tsx scripts/cleanup-stripe.ts",
    "see-active": "ts-node scripts/see-active.ts",
    "tunnel": "npx localtunnel --port 3000 --subdomain mahad-dev",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "webhook": "stripe listen --forward-to localhost:3000/api/webhook --events customer.subscription.created,customer.subscription.updated,customer.subscription.deleted,invoice.payment_succeeded,invoice.payment_failed,payment_intent.payment_failed",
    "dev:webhook": "concurrently \"npm run dev\" \"npm run webhook\"",
    "seed": "tsx prisma/seed.ts",
    "seed:checkins": "tsx prisma/seed-checkins.ts",
    "unseed:checkins": "tsx prisma/unseed-checkins.ts",
    "pr-template": "node .github/scripts/suggest-pr-template.js",
    "setup-hooks": "bash .github/scripts/setup-hooks.sh",
    "db:safety-check": "tsx scripts/db-safety-check.ts",
    "db:reset-safe": "tsx scripts/safe-migrate-reset.ts",
    "merge:duplicates": "tsx scripts/merge-duplicate-persons.ts"
  },
